<% if list.completed %>
<tr class="table-secondary"  id="<%= dom_id list %>">
<%else%>
<tr class="table-primary"  id="<%= dom_id list %>">
<%end%>
  <td>
    <%= list.description %>
  </td>
  <td>
    <%= list.string %>
  </td>
  <td>
    <%= list.completed %>
  </td>
  <td>
    <%= link_to "Show this list", list %>
    <%= link_to "Edit", edit_list_path(list) %>
    <%= button_to "Delete", list, method: :delete, data: {confirm: "Are you sure?"} %>
  </td>
</tr>
